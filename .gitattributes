class Car:
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def get_info(self):
        return f"{self.year} {self.make} {self.model}"

# Приклад використання
car = Car("Toyota", "Corolla", 2020)
print(car.get_info())  # Виведе: 2020 Toyota Corolla
